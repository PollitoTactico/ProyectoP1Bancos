@model IEnumerable<ProyectoP1Bancos.Models.ObjetoDes>

@{
    Layout = "~/Views/Shared/_LayoutHetero.cshtml";
    ViewData["Title"] = "Index";
}

<h1>Objeto que Deseas</h1>

<p>
    <div>
        <a asp-action="Create">Create New</a>
    </div>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreObjeto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Precio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreObjeto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Precio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdObjeto">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdObjeto">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdObjeto">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
