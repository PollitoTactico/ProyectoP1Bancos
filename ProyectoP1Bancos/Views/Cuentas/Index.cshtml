@model IEnumerable<ProyectoP1Bancos.Models.Cuenta>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <div>
        <a asp-action="Create">Create New</a>
    </div>
    
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NumCuenta)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NumCuenta)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdCuenta">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdCuenta">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdCuenta">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
